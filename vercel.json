{
    "version": 2,
    "builds": [
        { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [{
            "src": "/user/upload/photo/:id",
            "methods": ["PUT"],
            "dest": "/app.js"
        },
        {
            "src": "/user/photo/:id",
            "methods": ["GET"],
            "dest": "/app.js"
        },
        {
            "src": "/customer/emergencyreq/:id_vet/:id_pet",
            "methods": ["POST"],
            "dest": "/app.js"
        },
        {
            "src": "/vet/emergencyreq/:id",
            "methods": ["GET"],
            "dest": "/app.js"
        },
        {
            "src": "/vet/upload/photo/:id_vet",
            "methods": ["POST"],
            "dest": "/app.js"
        },
        {
            "src": "/vet/photo/:id_vet",
            "methods": ["GET"],
            "dest": "/app.js"
        },
        {
            "src": "/vet/upload/doc/:id_vet",
            "methods": ["POST"],
            "dest": "/app.js"
        },
        {
            "src": "/vet/show/doc/:id_vet",
            "methods": ["GET"],
            "dest": "/app.js"
        },
        {
            "src": "/.*",
            "dest": "/app.js"
        }
    ]
}